{
    "version": 2,
    "builds": [
      {
        "src": "api/websocket.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist" 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/websocket.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1" 
      }
    ]
  }
  